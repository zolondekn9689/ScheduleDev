<?xml version="1.0" encoding="UTF-8"?>

<?import java.lang.*?>
<?import java.util.*?>
<?import javafx.scene.*?>
<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>

<BorderPane xmlns="http://javafx.com/javafx"
            xmlns:fx="http://javafx.com/fxml"
            fx:controller="EmployeeInfoPage.EmployeeInfoPageController">
    <top>
        <MenuBar fx:id="menuBar">

            <menus>
                <Menu text="File">
                    <items>
                        <MenuItem text="Reset" />
                    </items>
                </Menu>
                <Menu text="Page">
                    <items>
                        <MenuItem text="Home" onAction="#menuActionHome"/>
                        <MenuItem text="Calendar Developer" onAction="#menuActionCal"/>
                        <MenuItem text="Employee Chart" onAction="#menuActionEmployeeChart"/>
                        <MenuItem text="Employee Info" onAction="#menuActionEmployeeInfo"/>
                    </items>
                </Menu>

            </menus>
        </MenuBar>
    </top>
</BorderPane>
